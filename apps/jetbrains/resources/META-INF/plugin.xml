<idea-plugin>
  <id>atom-one</id>
  <name>Atom One Theme</name>
  <vendor url="https://subtheme.pro">Subtheme</vendor>
  <version>1.1</version>

  <depends>com.intellij.modules.lang</depends>
  <idea-version since-build="191.0"/>

  <change-notes><![CDATA[
    <h3>1.1</h3>
    <ul>
      <li>Fixed counter foreground color</li>
    </ul>
    <h3>1.0</h3>
    <ul>
      <li>Fixed application border color in Linux</li>
      <li>Fixed suggestion hint selected line color</li>
    </ul>
  ]]>
  </change-notes>

  <description><![CDATA[
    The Atom One theme, adapted for JetBrains by <a href="https://subtheme.pro">Subtheme</a>. Both dark and light themes are included.
    <br/><br/>
    Contribute with <a href="https://github.com/subtheme-pro/atom-one">GitHub</a>. Screenshots taken with <a href="https://plugins.jetbrains.com/plugin/10044-atom-material-icons">Atom Material Icons</a> and <a href="https://www.typography.com/fonts/operator/styles">Operator Mono</a>.
    <br/><br/>
    <img src="https://raw.githubusercontent.com/subtheme-pro/atom-one/master/apps/jetbrains/screenshots/atom-one-dark-small.png" border="0" width="360">
    <br/>
    <img src="https://raw.githubusercontent.com/subtheme-pro/atom-one/master/apps/jetbrains/screenshots/atom-one-light-small.png" border="0" width="360">
  ]]>
  </description>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="246a798b-a742-42c0-a587-43f3aceb11bb" path="/atom-one-dark.theme.json"/>
    <themeProvider id="60306159-29af-468e-9c4d-d7d312c47a99" path="/atom-one-light.theme.json"/>
  </extensions>
</idea-plugin>